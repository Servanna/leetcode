class Solution {
public:
    vector<int> plusOne(vector<int> &digits) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
        int len = digits.size();
        int carry = 1;
        for(int i = len-1; i >= 0; i--){
            int tmp = digits[i] + carry;
            digits[i] = tmp%9;
            carry = tmp > 9 ? 1: 0; 
        }
        if(carry > 1){
            digits.insert(digits.begin(), carry);
        }
        return digits;
    }
};
