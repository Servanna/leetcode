class Solution {
public:
    int reverse(int x) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
        int orig = x;
        int ret = 0;
        int d = 0;
        while(x != 0){
            d = x%10;
            x = x/10;
            ret += ret*10 + d;
        }
        return ret;
    }
};
